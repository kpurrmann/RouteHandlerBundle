{
	"name": "purrmannwebsolutions/route-note-bundle",
	"description": "This Symfony bundle will allow you to create notes for every configured route.",
	"keywords": ["Symfony", "note", "route"],
	"homepage": "https://github.com/kpurrmann/RouteNoteBundle",
	"type": "symfony-bundle",
	"license": "MIT",
	"authors": [
		{
			"name": "Kevin Purrmann",
			"email": "kevin@purrmann-websolutions.de"
		}
	],
	"require": {
		"php": "^7.1",
		"symfony/framework-bundle": "^3.4|^4.0|^5.0",
		"symfony/twig-bundle": "^3.4|^4.0|^5.0",
		"symfony/form": "^3.4|^4.0|^5.0",
		"doctrine/orm": "^2.4.8|^2.5",
		"doctrine/doctrine-bundle": "^1.4|^1.6|^2.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^7.0|^8.0",
		"php-coveralls/php-coveralls": "^2.0",
		"matthiasnoback/symfony-dependency-injection-test": "^3.1|^4.1"
	},
	"autoload": {
		"psr-4": {
			"PurrmannWebsolutions\\RouteNoteBundle\\": ""
		}
	},
	"autoload-dev": {
		"psr-4": {
			"PurrmannWebsolutions\\RouteNoteBundle\\Tests\\": "Tests/"
		}
	}
}
